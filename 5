buk = input("Введите столбец")
num = int(input("Введите строку"))
if buk == "b" or buk == "d" or buk == "f" or buk == "h":
    if num == 1 or num == 3 or num == 5 or num == 7:
        print ("Белая клетка")
    else: print ("Черная клетка")
elif buk == "a" or buk == "c" or buk == "e" or buk == "g":
    if num == 2 or num == 4 or num == 6 or num == 8:
        print ("Белая клетка")
    else: print ("Черная клетка")
